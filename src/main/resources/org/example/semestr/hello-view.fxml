    <Button layoutX="470.0" fx:id="buttonLogin" layoutY="283.0" mnemonicParsing="false" onAction="#connect" text="Подключиться" />
    <TextField layoutX="426.0" layoutY="238.0" fx:id="usernameField" />
    <Label layoutX="452.0" fx:id="helloLabel" layoutY="197.0" text="Введите свой ник" />
    <Button layoutX="200.0" fx:id="buttonSendMessage" layoutY="500.0" mnemonicParsing="false" onAction="#sendMessage" visible="false" text="Отправить сообщение" />
